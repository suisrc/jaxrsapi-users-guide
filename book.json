{
  "gitbook": "3.x.x",
  "structure": {
      "readme": "README.adoc"
  },
  "plugins": [
      "ungrey",
      "splitter",
      "expandable-chapters",
      "edit-link",
      "addcssjs",
      "ga"
  ],
  "pluginsConfig": {
          "edit-link": {
             "base": "https://github.com/suisrc/jaxrsapi-users-guide/tree/master/",
             "label": "编辑"
          },
          "addcssjs": {
              "css": ["/styles/asciidoc.css"]
          },
          "ga": {
              "token": "UA-83419890-1"
          }
   },
  "variables": {
      "javadoc_url_preffix" : "http://suisrc.github.io/weixin-api/",
      "versions": {
          "wildfly": "10.1.0.Final"
      }
  }

}
